    "@sentry/nextjs": "7.120.0",
    "@sentry/react": "7.120.0",
    "@sentry/tracing": "7.120.3",
    "@tanstack/react-query": "^5.76.1",
    "@types/fs-extra": "^11.0.4",
    "@types/iframe-resizer": "^3.5.13",
