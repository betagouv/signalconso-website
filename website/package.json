    "@types/react-dom": "18.3.1",
    "@types/react-swipeable-views": "0.13.5",
    "@types/smoothscroll-polyfill": "0.3.4",
    "babel-jest": "29.7.0",
    "concurrently": "^8.2.2",
    "cross-env": "^7.0.3",
    "eslint": "8.57.1",
    "eslint-config-next": "^14.2.18",
    "jest": "29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "jest-html-reporters": "3.1.7",
    "nodemon": "^3.1.7",
    "prettier": "3.2.5",
    "sass": "^1.77.2",
    "ts-node": "10.9.2",
    "typescript": "5.4.5"
  }
}
