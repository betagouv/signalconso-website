{
  "name": "signalconso-website",
  "version": "0.1.0",
  "private": true,
  "engines": {
    "node": "20.9.0"
  },
  "scripts": {
    "beforebuild": "echo 'beforebuild starting' && yarn generate-json && yarn generate-sitemap  && only-include-used-icons && echo 'beforebuild done' ",
    "dev": "yarn beforebuild && next dev -p 3001",
    "devdemo": "yarn beforebuild && NEXT_PUBLIC_API_BASE_URL=https://demo-signalement-api.cleverapps.io next dev -p 3001",
    "build": "yarn beforebuild && tsc --declaration  && next build",
    "runplaywright": "npx browserslist@latest --update-db --yes && npx playwright install --with-deps && npx playwright test",
    "start": "next start",
    "lint": "next lint",
    "test": "jest",
    "test-no-html-output": "DEBUG_PRINT_LIMIT=1 jest",
    "test-dev": "jest --config=jest.config.ts --watch",
    "generate-json": "ts-node -O '{\"module\":\"commonjs\", \"jsx\":\"react-jsx\"}' src/scripts/gen-anomalies-json.ts",
    "gen-satisfaction-data": "ts-node -O '{\"module\":\"commonjs\", \"jsx\":\"react-jsx\"}' src/scripts/gen-satisfaction-data.tsx",
    "generate-minimized-anomalies": "ts-node -O '{\"module\":\"commonjs\", \"jsx\":\"react-jsx\"}' src/scripts/gen-minimized-anomalies.ts",
    "generate-landings": "ts-node -O '{\"module\":\"commonjs\", \"jsx\":\"react-jsx\"}' src/scripts/generateLandingDataFromAirtable.ts && prettier --write src/landings",
    "generate-sitemap": "ts-node -O '{\"module\":\"commonjs\", \"jsx\":\"react-jsx\"}' src/scripts/gen-sitemap.ts",
    "sanitize-anomalies-filenames": "ts-node -O '{\"module\":\"commonjs\", \"jsx\":\"react-jsx\"}' src/scripts/sanitize-anomalies-filenames.ts",
    "format": "prettier --write .",
    "checkformat": "prettier --check .",
    "checktypes": "tsc --noEmit",
    "checkall": "yarn checktypes && yarn checkformat && yarn lint && yarn test",
    "i18nToCSV": "ts-node -O '{\"module\":\"commonjs\", \"jsx\":\"react-jsx\"}' src/scripts/i18nToCSV.ts"
  },
  "heroku-run-build-script": true,
  "dependencies": {
    "@codegouvfr/react-dsfr": "^0.78.6",
    "@emotion/cache": "11.11.0",
    "@emotion/react": "11.11.1",
    "@emotion/server": "11.11.0",
    "@emotion/styled": "11.11.0",
    "@formatjs/intl-localematcher": "^0.5.2",
    "@headlessui/react": "^1.7.17",
    "@headlessui/tailwindcss": "^0.2.0",
    "@hello-pangea/dnd": "^16.3.0",
    "@iarna/toml": "^2.2.5",
    "@material-ui/core": "^4.12.4",
    "@material-ui/icons": "^4.11.3",
    "@react-hook/throttle": "^2.2.0",
    "@sentry/nextjs": "7.81.0",
    "@sentry/react": "7.81.0",
    "@sentry/tracing": "7.81.0",
    "@tanstack/react-query": "^5.8.4",
    "@types/fs-extra": "^11.0.4",
    "@types/iframe-resizer": "^3.5.13",
    "@types/lodash.throttle": "^4.1.9",
    "@types/uuid": "^9.0.7",
    "airtable": "^0.12.2",
    "autoprefixer": "^10.4.16",
    "axios": "1.6.2",
    "browser-image-compression": "2.0.2",
    "cookies-next": "^4.1.0",
    "csv-parse": "^5.5.2",
    "date-fns": "2.30.0",
    "fuse.js": "^7.0.0",
    "heic2any": "^0.0.4",
    "iframe-resizer": "^4.3.9",
    "jest": "29.7.0",
    "js-yaml": "4.1.0",
    "lodash": "4.17.21",
    "negotiator": "^0.6.3",
    "next": "^14.0.3",
    "postcss": "^8.4.31",
    "qs": "6.11.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "7.48.2",
    "react-imask": "7.1.3",
    "react-swipeable-views": "0.14.0",
    "recharts": "2.10.1",
    "rimraf": "^5.0.5",
    "sharp": "^0.32.6",
    "slugify": "^1.6.6",
    "smoothscroll-polyfill": "0.4.4",
    "tailwindcss": "^3.3.5",
    "tss-react": "^4.9.3",
    "uuid": "^9.0.1",
    "yaml-import": "2.0.0"
  },
  "devDependencies": {
    "@jest/types": "29.6.3",
    "@playwright/test": "^1.41.0",
    "@testing-library/dom": "9.3.3",
    "@testing-library/jest-dom": "6.1.4",
    "@testing-library/react": "14.1.2",
    "@testing-library/user-event": "14.5.1",
