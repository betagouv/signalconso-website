{
  "name": "signalconso-website",
  "version": "0.1.0",
  "private": true,
  "engines": {
    "node": "16.15.1"
  },
  "scripts": {
    "dev": "yarn generate-json && next dev -p 3001",
    "devdemo": "yarn generate-json && NEXT_PUBLIC_API_BASE_URL=https://demo-signalement-api.cleverapps.io next dev -p 3001",
    "build": "yarn generate-json && tsc --declaration  && next build",
    "start": "next start",
    "lint": "next lint",
    "test": "jest",
    "test-no-html-output": "DEBUG_PRINT_LIMIT=1 jest",
    "test-dev": "jest --config=jest.config.ts --watch",
    "generate-json": "ts-node -O \"{\\\"module\\\":\\\"commonjs\\\"}\" src/anomaly/script/generate-json-files.ts",
    "format": "npx prettier --write .",
    "checkformat": "npx prettier --check ."
  },
  "heroku-run-build-script": true,
