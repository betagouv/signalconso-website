{
  "name": "signalconso-website",
  "version": "0.1.0",
  "private": true,
  "engines": {
    "node": "16.15.1"
  },
  "scripts": {
    "beforebuild": "echo 'beforebuild starting' && yarn generate-json && yarn generate-sitemap && only-include-used-icons && echo 'beforebuild done'",
    "dev": "yarn beforebuild && next dev -p 3001",
    "devdemo": "yarn beforebuild && NEXT_PUBLIC_API_BASE_URL=https://demo-signalement-api.cleverapps.io next dev -p 3001",
    "build": "yarn beforebuild && tsc --declaration  && next build",
    "start": "next start",
    "lint": "next lint",
    "test": "jest",
    "test-no-html-output": "DEBUG_PRINT_LIMIT=1 jest",
    "test-dev": "jest --config=jest.config.ts --watch",
    "generate-json": "ts-node -O '{\"module\":\"commonjs\", \"jsx\":\"react-jsx\"}' src/scripts/gen-anomalies-json.ts",
    "generate-minimized-anomalies": "ts-node -O '{\"module\":\"commonjs\", \"jsx\":\"react-jsx\"}' src/scripts/gen-minimized-anomalies.ts",
    "generate-landings": "ts-node -O '{\"module\":\"commonjs\", \"jsx\":\"react-jsx\"}' src/scripts/generateLandingDataFromAirtable.ts",
    "generate-sitemap": "ts-node -O '{\"module\":\"commonjs\", \"jsx\":\"react-jsx\"}' src/scripts/gen-sitemap.ts",
    "sanitize-anomalies-filenames": "ts-node -O '{\"module\":\"commonjs\", \"jsx\":\"react-jsx\"}' src/scripts/sanitize-anomalies-filenames.ts",
    "format": "prettier --write .",
    "checkformat": "prettier --check .",
    "checktypes": "tsc --noEmit",
    "checkall": "yarn checktypes && yarn checkformat && yarn lint && yarn test",
    "i18nToCSV": "ts-node -O '{\"module\":\"commonjs\", \"jsx\":\"react-jsx\"}' src/scripts/i18nToCSV.ts"
  },
  "heroku-run-build-script": true,
  "dependencies": {
    "@codegouvfr/react-dsfr": "^0.75.3",
    "@emotion/cache": "11.11.0",
    "@emotion/react": "11.11.1",
    "@emotion/server": "11.11.0",
    "@emotion/styled": "11.11.0",
    "@formatjs/intl-localematcher": "^0.4.0",
    "@iarna/toml": "^2.2.5",
    "@material-ui/core": "^4.12.4",
    "@material-ui/icons": "^4.11.3",
    "@mui/icons-material": "^5.14.1",
    "@mui/lab": "5.0.0-alpha.137",
    "@mui/material": "5.14.2",
    "@mui/styled-engine": "5.13.2",
    "@mui/system": "5.14.1",
    "@react-hook/throttle": "^2.2.0",
    "@sentry/nextjs": "7.60.0",
    "@sentry/react": "7.60.0",
    "@sentry/tracing": "7.60.0",
    "@tanstack/react-query": "^4.32.0",
    "@types/fs-extra": "^11.0.1",
    "@types/lodash.throttle": "^4.1.7",
    "@types/uuid": "^9.0.2",
    "airtable": "^0.12.1",
    "autoprefixer": "^10.4.14",
    "axios": "1.4.0",
    "browser-image-compression": "2.0.2",
    "cookies-next": "^2.1.2",
    "csv-parse": "^5.4.0",
    "date-fns": "2.30.0",
    "fuse": "^0.4.0",
    "fuse.js": "^6.6.2",
    "heic2any": "^0.0.4",
    "jest": "29.6.1",
    "js-yaml": "4.1.0",
    "lodash": "4.17.21",
    "negotiator": "^0.6.3",
    "next": "latest",
    "postcss": "^8.4.27",
    "qs": "6.11.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "7.45.2",
    "react-imask": "7.1.3",
    "react-swipeable-views": "0.14.0",
    "recharts": "2.7.2",
    "rimraf": "^5.0.1",
    "sharp": "^0.32.4",
    "slugify": "^1.6.6",
    "smoothscroll-polyfill": "0.4.4",
    "tailwindcss": "^3.3.3",
    "tss-react": "^4.8.8",
    "uuid": "^9.0.0",
    "yaml-import": "2.0.0"
  },
  "devDependencies": {
    "@jest/types": "29.6.1",
    "@testing-library/dom": "9.3.1",
    "@testing-library/jest-dom": "5.17.0",
    "@testing-library/react": "14.0.0",
    "@testing-library/user-event": "14.4.3",
    "@types/date-fns": "2.6.0",
    "@types/jest": "29.5.3",
    "@types/js-yaml": "^4.0.5",
    "@types/lodash": "4.14.195",
    "@types/lodash.uniqby": "^4.7.7",
    "@types/negotiator": "^0.6.1",
    "@types/node": "20.4.4",
    "@types/qs": "^6.9.7",
    "@types/randomstring": "1.1.8",
    "@types/react": "18.2.16",
    "@types/react-dom": "18.2.7",
    "@types/react-swipeable-views": "0.13.2",
    "@types/smoothscroll-polyfill": "0.3.1",
    "babel-jest": "29.6.1",
    "eslint": "8.45.0",
    "eslint-config-next": "^13.4.12",
    "jest": "29.6.1",
    "jest-environment-jsdom": "^29.6.1",
    "jest-html-reporters": "3.1.4",
    "prettier": "3.0.0",
    "ts-jest": "29.1.1",
    "ts-node": "10.9.1",
    "typescript": "5.1.6"
  },
  "packageManager": "yarn@3.2.1"
}
