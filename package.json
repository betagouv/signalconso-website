    "@types/react": "18.2.38",
    "@types/react-dom": "18.2.16",
    "@types/react-swipeable-views": "0.13.5",
    "@types/smoothscroll-polyfill": "0.3.3",
    "babel-jest": "29.7.0",
    "eslint": "8.54.0",
    "eslint-config-next": "^14.0.3",
    "jest": "29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "jest-html-reporters": "3.1.4",
    "prettier": "3.1.0",
    "sass": "^1.69.5",
    "ts-jest": "29.1.1",
    "ts-node": "10.9.1",
    "typescript": "5.3.2"
  },
  "packageManager": "yarn@3.2.1"
}
