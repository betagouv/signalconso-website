    "jest": "28.1.3",
    "js-yaml": "4.1.0",
    "lodash": "4.17.21",
    "next": "12.2.5",
    "next-transpile-modules": "^9.0.0",
    "qs": "6.11.0",
    "randomstring": "1.2.2",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-hook-form": "7.34.2",
    "react-imask": "6.4.2",
    "react-swipeable-views": "0.14.0",
