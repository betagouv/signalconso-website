{
  "name": "signalconso-website",
  "version": "0.1.0",
  "private": true,
  "engines": {
    "node": "16.15.1"
  },
  "scripts": {
    "beforebuild": "echo 'beforebuild starting' && yarn generate-json && yarn generate-sitemap && only-include-used-icons && echo 'beforebuild done'",
    "dev": "yarn beforebuild && next dev -p 3001",
    "devdemo": "yarn beforebuild && NEXT_PUBLIC_API_BASE_URL=https://demo-signalement-api.cleverapps.io next dev -p 3001",
    "build": "yarn beforebuild && tsc --declaration  && next build",
    "start": "next start",
    "lint": "next lint",
    "test": "jest",
    "test-no-html-output": "DEBUG_PRINT_LIMIT=1 jest",
    "test-dev": "jest --config=jest.config.ts --watch",
    "generate-json": "ts-node -O \"{\\\"module\\\":\\\"commonjs\\\"}\" src/anomalies/script/generate-json-files.ts",
    "generate-landings": "ts-node -O \"{\\\"module\\\":\\\"commonjs\\\"}\" src/landings/script/generateLandingDataFromCsv.ts",
    "generate-sitemap": "ts-node -O \"{\\\"module\\\":\\\"commonjs\\\"}\" src/scripts/generate-sitemap.ts",
    "gen-tmp": "ts-node -O \"{\\\"module\\\":\\\"commonjs\\\"}\" src/anomalies/script/gen-tmp.ts",
    "format": "prettier --write .",
    "checkformat": "prettier --check .",
    "checktypes": "tsc --noEmit",
    "checkall": "yarn checktypes && yarn checkformat && yarn test"
  },
  "heroku-run-build-script": true,
  "dependencies": {
    "@codegouvfr/react-dsfr": "^0.45.0",
    "@emotion/cache": "11.10.1",
    "@emotion/react": "11.10.0",
    "@emotion/server": "11.10.0",
    "@emotion/styled": "11.10.0",
    "@mui/lab": "5.0.0-alpha.95",
    "@mui/material": "5.10.1",
    "@mui/styled-engine": "5.10.1",
    "@mui/system": "5.10.1",
    "@next/bundle-analyzer": "^13.2.4",
    "@sentry/nextjs": "7.10.0",
    "@sentry/react": "7.10.0",
    "@sentry/tracing": "7.10.0",
    "@tanstack/react-query": "^4.26.1",
    "@types/lodash.throttle": "^4.1.7",
    "autoprefixer": "^10.4.14",
    "axios": "0.27.2",
    "browser-image-compression": "2.0.0",
    "csv-parse": "^5.3.8",
    "date-fns": "2.29.1",
    "heic2any": "^0.0.3",
    "jest": "28.1.3",
    "js-yaml": "4.1.0",
    "lodash": "4.17.21",
    "next": "latest",
    "postcss": "^8.4.21",
    "qs": "6.11.0",
    "react": "latest",
    "react-dom": "latest",
    "react-hook-form": "7.34.2",
    "react-imask": "6.4.2",
    "react-swipeable-views": "0.14.0",
    "recharts": "2.1.12",
    "rimraf": "^5.0.0",
    "slugify": "^1.6.6",
    "smoothscroll-polyfill": "0.4.4",
    "tailwindcss": "^3.2.7",
    "tss-react": "^4.8.2",
    "yaml-import": "2.0.0"
  },
  "devDependencies": {
    "@jest/types": "28.1.3",
    "@testing-library/dom": "8.17.1",
    "@testing-library/jest-dom": "5.16.5",
    "@testing-library/react": "13.3.0",
    "@testing-library/user-event": "14.4.3",
    "@types/date-fns": "2.6.0",
    "@types/jest": "28.1.7",
    "@types/js-yaml": "^4.0.5",
    "@types/lodash": "4.14.183",
    "@types/lodash.uniqby": "^4.7.7",
    "@types/node": "18.7.6",
    "@types/qs": "^6.9.7",
    "@types/randomstring": "1.1.8",
    "@types/react": "18.0.17",
    "@types/react-dom": "18.0.6",
    "@types/react-swipeable-views": "0.13.1",
    "@types/smoothscroll-polyfill": "0.3.1",
    "babel-jest": "28.1.3",
    "eslint": "8.22.0",
    "eslint-config-next": "latest",
    "jest": "28.1.3",
    "jest-environment-jsdom": "^28.1.3",
    "jest-html-reporters": "3.0.10",
    "prettier": "2.7.1",
    "ts-jest": "28.0.8",
    "ts-node": "10.9.1",
    "typescript": "4.7.4"
  },
  "packageManager": "yarn@3.2.1"
